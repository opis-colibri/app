{
    "name": "opis-colibri/app",
    "type": "project",
    "description": "Default application for Opis Colibri framework",
    "keywords": ["framework", "php", "opis", "opis colibri", "app", "application"],
    "homepage": "http://opis.io",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Marius Sarca",
            "email": "marius.sarca@gmail.com"
        }
    ],
    "repositories": [
        {"type": "path", "url": "/Users/msarca/code/opis-colibri/welcome"}
    ],
    "require": {
        "filp/whoops": "2.*",
        "opis-colibri/welcome": "master-dev as 3.0.x-dev"
    },
    "config": {
        "preferred-install": "dist"
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "scripts": {
        "post-create-project-cmd": [
            "@php -r \"rename('storage' . DIRECTORY_SEPARATOR . '.bootstrap.php', 'storage' . DIRECTORY_SEPARATOR . 'bootstrap.php');\"",
            "@php colibri install --enable opis-colibri/welcome"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
