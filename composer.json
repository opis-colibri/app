{
    "name": "opis-colibri/app",
    "type": "project",
    "description": "Default application for Opis Colibri framework",
    "keywords": ["framework", "php", "opis", "opis colibri", "app", "application"],
    "homepage": "http://opis.io",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Marius Sarca",
            "email": "marius.sarca@gmail.com"
        }
    ],
    "require": {
        "opis/colibri": "2.0.*@dev",
        "opis-colibri/install": "3.0.*@dev",
        "opis-colibri/system": "3.0.*@dev",
        "opis-colibri/manager": "3.0.*@dev",
        "opis-colibri/commands": "3.0.*@dev",
        "opis-colibri/welcome": "3.0.*@dev"
    },
    "config": {
        "preferred-install": "dist"
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "scripts": {
        "pre-autoload-dump": [
            "Opis\\Colibri\\Composer\\EventHandler::onDumpAutoload"
        ]
    },
    "extra": {
        "application": {
            "installer": "opis-colibri/install",
            "modules": ["opis-colibri/welcome", "opis-colibri/commands"]
        },
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
