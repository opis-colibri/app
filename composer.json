{
    "name": "opis-colibri/app",
    "type": "project",
    "description": "The application shell of the Opis Colibri framework",
    "keywords": [ "opis colibri", "framework", "php", "opis"],
    "homepage": "https://opis.io",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Marius Sarca",
            "email": "marius.sarca@gmail.com"
        },
        {
            "name": "Sorin Sarca",
            "email": "sarca_sorin@hotmail.com"
        }
    ],
    "repositories": {
        "local": {
            "type": "path",
            "url": "./local/*"
        }
    },
    "require": {
        "php": "^8.0",
        "opis-colibri/core": "2020.x-dev"
    },
    "require-dev": {
        "filp/whoops": "2.*"
    },
    "config": {
        "process-timeout": 0,
        "preferred-install": "dist"
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "2020.x-dev"
        }
    }
}
